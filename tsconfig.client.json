{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "sourceMap": false,
        "outDir": "lib/client",
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "lib": [
            "dom",
            "es5"
        ]
    },
    "include": [
        "src/client/**/*.ts",
        "src/types/**/*.d.ts"
    ],
    "exclude": [
        "src/client/**/*.spec.ts"
    ]
}